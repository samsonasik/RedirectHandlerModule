{
    "name": "samsonasik/redirect-handler-module",
    "type": "library",
    "description": "ZF2 module to for Url Redirect handling",
    "keywords": ["redirect","zf2","zf3","handler"],
    "homepage": "https://github.com/samsonasik/RedirectHandlerModule",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Malik Ikhsan",
            "email": "samsonasik@gmail.com",
            "homepage": "http://samsonasik.wordpress.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-servicemanager": "^3.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "satooshi/php-coveralls": "^2.0",
        "zendframework/zend-coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RedirectHandlerModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RedirectHandlerModuleTest\\": "test/"
        }
    },
    "config": {
        "process-timeout": 5000
    },
    "extra": {
        "zf": {
            "module": "RedirectHandlerModule"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
